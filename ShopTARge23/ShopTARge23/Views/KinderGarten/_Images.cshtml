@model List<ShopTARge23.Models.KinderGarten.KinderGartenImageViewModel>

@if (Model != null && Model.Any())

{
    <div class="row">
        @foreach (var image in Model)

        {
            <div class="col-md-3">
                <img src="data:image;base64,@Convert.ToBase64String(image.ImageData)" class="img-thumbnail" alt="@image.ImageTitle" />
                <p>@image.ImageTitle</p>
            </div>
        }
    </div>
}

else

{
    <p>No images available.</p>
}
